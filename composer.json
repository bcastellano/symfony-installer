{
    "name": "bcastellano/symfony-installer",
    "description": "Symfony installer to create regular projects and multiple application projects.",
    "keywords": ["symfony", "framework", "installer", "php", "multiple application"],
    "license": "MIT",
    "authors": [
        {
            "name": "Blas Castellano Moreno",
            "email": "b.castellano.moreno@gmail.com",
            "homepage": "http://bcastellano.es"
        }
    ],
    "autoload": {
        "psr-4": { "Symfony\\": "src/Symfony/" }
    },
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "~5.0",
        "symfony/console": "~2.6",
        "symfony/filesystem": "~2.5",
        "raulfraile/distill": "~0.9,!=0.9.3,!=0.9.4"
    },
    "require-dev": {
        "symfony/process": "~2.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "bin": [
        "symfony"
    ]
}
